<scpd xmlns="urn:schemas-upnp-org:service-1-0">
	<specVersion>
		<major>
			1
		</major>
		<minor>
			0
		</minor>
	</specVersion>
	<actionList>
		<action>
			<name>
				GetBatteryLevel
			</name>
			<argumentList>
				<argument>
					<name>
						Battery
					</name>
					<direction>
						out
					</direction>
					<retval/>
					<relatedStateVariable>
						BatteryLevel
					</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>
				GetStillTime
			</name>
			<argumentList>
				<argument>
					<name>
						Still
					</name>
					<direction>
						out
					</direction>
					<retval/>
					<relatedStateVariable>
						StillTime
					</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>
				GetTimeStamp
			</name>
			<argumentList>
				<argument>
					<name>
						Timestamp
					</name>
					<direction>
						out
					</direction>
					<retval/>
					<relatedStateVariable>
						TimeStamp
					</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
	</actionList>
	<serviceStateTable>
		<stateVariable sendEvents="no">
			<name>
				TimeStamp
			</name>
			<dataType>
				i4
			</dataType>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>
				BatteryLevel
			</name>
			<dataType>
				ui4
			</dataType>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>
				StillTime
			</name>
			<dataType>
				ui4
			</dataType>
		</stateVariable>
	</serviceStateTable>
</scpd>
