<?xml version="1.0" encoding="utf-8"?>
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
   <specVersion>
      <major>1</major>
      <minor>0</minor>
   </specVersion>
   <actionList>
      <action>
         <name>SetBiefNumber</name>
         <argumentList>
            <argument>
               <name>bief</name>
               <direction>in</direction>
               <relatedStateVariable>number</relatedStateVariable>
            </argument>
         </argumentList>
      </action>
      <action>
         <name>SetKmlData</name>
         <argumentList>
            <argument>
               <name>xmlData</name>
               <direction>in</direction>
               <relatedStateVariable>Words</relatedStateVariable>
            </argument>
         </argumentList>
      </action>
      <action>
         <name>setKmlFile</name>
         <argumentList>
            <argument>
               <name>uri</name>
               <direction>in</direction>
               <relatedStateVariable>Words</relatedStateVariable>
            </argument>
         </argumentList>
      </action>
      <action>
         <name>SetLayers</name>
         <argumentList>
            <argument>
               <name>layers</name>
               <direction>in</direction>
               <relatedStateVariable>Words</relatedStateVariable>
            </argument>
         </argumentList>
      </action>
      <action>
         <name>setMap</name>
         <argumentList>
            <argument>
               <name>url</name>
               <direction>in</direction>
               <relatedStateVariable>Words</relatedStateVariable>
            </argument>
         </argumentList>
      </action>
      <action>
         <name>SwitchToExternalKMLLink</name>
         <argumentList>
            <argument>
               <name>useExternalLink</name>
               <direction>in</direction>
               <relatedStateVariable>boolette</relatedStateVariable>
            </argument>
         </argumentList>
      </action>
   </actionList>
   <serviceStateTable>
      <stateVariable sendEvents="no">
         <name>Words</name>
         <dataType>string</dataType>
      </stateVariable>
      <stateVariable sendEvents="no">
         <name>boolette</name>
         <dataType>boolean</dataType>
      </stateVariable>
      <stateVariable sendEvents="no">
         <name>number</name>
         <dataType>i4</dataType>
      </stateVariable>
   </serviceStateTable>
</scpd>